<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jon.electronicsengineeringtoolkit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OhmActivity"
            android:label="@string/title_activity_ohm">
        </activity>
        <activity
            android:name=".ColorActivity"
            android:label="@string/title_activity_color">
        </activity>
        <activity
            android:name=".FormulaActivity"
            android:label="@string/title_activity_formula">
        </activity>
    </application>

</manifest>